<h1>This is the filebrowser</h1>

<%= form_for(@rich_image, :url => files_path, :html => { :multipart => true }) do |f| %>
  <p><%= f.file_field :image%></p>
  <%= f.submit("Upload") %>
<% end %>

<div class="test">
  
</div>

<ul>
<% for image in @images %>
    <li><%= image_tag(image.image.url(:thumb)) %></li>
<% end %>
</ul>